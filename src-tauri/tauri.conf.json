{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "neko233-hardware-viewer",
    "version": "1.0.3"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/neko233-com/neko233-hardware-viewer/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU0NUIzNzY1QzlCOEYwQTkKUldTcDhMakpaVGRiNUJWY1ZzOHA5UlJSREtkYkY4cUtBQjJOdm8xdGZKVzQwVXZUVTE2cGZ2TEEK"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.neko233.hardwareviewer",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Neko233 Hardware Viewer",
        "width": 1200,
        "height": 800,
        "decorations": false,
        "transparent": true
      }
    ]
  }
}
